{
	"name": "eslint-config-chariz",
	"version": "1.0.0",
	"description": "ESLint config used at Chariz",
	"main": "dist/.eslintrc.js",
	"module": "dist/.eslintrc.mjs",
	"exports": {
		".": {
			"import": "./.eslintrc.mjs",
			"require": "./.eslintrc.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chariz/eslint.git"
	},
	"keywords": [
		"eslint",
		"eslintconfig",
		"esm",
		"react",
		"preact",
		"typescript",
		"es2020",
		"es2019",
		"es2018",
		"es2017",
		"es2016",
		"es2015",
		"es6",
		"es7",
		"es8",
		"ecma"
	],
	"author": "Aarnav Tale <tale@chariz.com>",
	"contributors": [
		"Adam Demasi <adam@chariz.com>"
	],
	"license": "Apache-2.0",
	"homepage": "https://github.com/chariz/eslint#readme",
	"scripts": {
		"push": "np",
		"prepublishOnly": "tsup ./.eslintrc.js --format cjs,esm --clean"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.7.0",
		"eslint-config-preact": "^1.3.0",
		"eslint-plugin-react": "^7.28.0",
		"jest": "^27.4.7"
	},
	"peerDependencies": {
		"eslint": ">= 8",
		"typescript": "*"
	},
	"devDependencies": {
		"np": "^7.6.0",
		"tsup": "^5.11.11",
		"typescript": "^4.5.5"
	}
}
