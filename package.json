{
	"name": "eslint-config-chariz",
	"version": "1.6.0",
	"type": "module",
	"description": "ESLint config used at Chariz",
	"author": "Adam Demasi <adam@chariz.com>",
	"contributors": [
		"Aarnav Tale <tale@chariz.com>"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"license": "Apache-2.0",
	"scripts": {
		"lint": "eslint *.js",
		"lint:fix": "eslint --fix *.js",
		"push": "np --no-cleanup --no-tests --message \"chore: v%s\"",
		"prepublishOnly": "tsup *.js --format cjs,esm --clean",
		"test": "pnpm -s lint"
	},
	"keywords": [
		"eslint",
		"eslintconfig",
		"esm",
		"react",
		"preact",
		"typescript",
		"es2026",
		"es2025",
		"es2024",
		"es2023",
		"es2022",
		"es2021",
		"es2020",
		"es2019",
		"es2018",
		"es2017",
		"es2016",
		"es2015",
		"es6",
		"es7",
		"es8"
	],
	"engines": {
		"node": ">=22"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chariz/eslint.git"
	},
	"bugs": {
		"url": "https://github.com/chariz/eslint/issues"
	},
	"homepage": "https://github.com/chariz/eslint#readme",
	"dependencies": {
		"@eslint/js": "^9.38.0",
		"eslint": "^9.38.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unicorn": "^62.0.0",
		"eslint-plugin-vue": "^10.5.1",
		"globals": "^16.4.0",
		"typescript-eslint": "^8.46.2",
		"vue-eslint-parser": "^10.2.0"
	},
	"peerDependencies": {
		"eslint": ">=9",
		"typescript": "*"
	},
	"devDependencies": {
		"@types/eslint": "^9.6.1",
		"np": "^10.2.0",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3"
	}
}
